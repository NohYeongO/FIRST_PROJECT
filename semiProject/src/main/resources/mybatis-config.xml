<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>



	<typeAliases>
		<typeAlias type="com.kh.semi.member.model.vo.Member" alias="member"/>
		<typeAlias type="com.kh.semi.customer.model.vo.Notice" alias="notice"/>
		<typeAlias type="com.kh.semi.customer.model.vo.QNA" alias="qna"/>
		<typeAlias type="com.kh.semi.customer.model.vo.NoticeFile" alias="noticeFile"/>
		<typeAlias type="com.kh.semi.customer.model.vo.Answer" alias="answer"/>
		<typeAlias type="com.kh.semi.common.model.vo.PageInfo" alias="pageInfo"/>
	</typeAliases>
	
	
	
	<mappers>
		<mapper resource="/mapper/member-mapper.xml"/>
		<mapper resource="/mapper/customer-mapper.xml"/>
	</mappers>



</configuration>